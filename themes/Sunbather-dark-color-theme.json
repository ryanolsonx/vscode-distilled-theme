  },
  "tokenColors": [
    {
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "foreground": "#4E4E4E"
      }
    },
    {
      "scope": ["string", "constant"],
      "settings": {
        "foreground": "#d75f87"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#767676"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#DC322F"
      }
    }
  ]
}
